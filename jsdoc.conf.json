{
    "source": {
        "include": ["src", "package.json", "README.md"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs)"
    },
    "plugins": ["plugins/markdown"],
    "opts": {
        "encoding": "utf8",
        "readme": "./README.md",
        "destination": "docs/",
        "recurse": true,
        "verbose": true,
        "template": "./node_modules/clean-jsdoc-theme",
        "theme_opts": {
            "base_url": "file:///Users/andrewdavis/Cardinal-designs/cardinal-designs/docs/Cardinal%20designs/0.1.0/index.html",
            "favicon": "../../../public/favicon.ico"
        }
    },
    "markdown": {
        "hardwrap": false,
        "idInHeadings": true
    }
}